    "prettier": "^2.6.0",
    "react-app-rewire-alias": "^1.0.0",
    "react-app-rewired": "^2.2.1",
    "sass": "^1.49.9",
    "ts-loader": "^9.2.8",
    "webpack": "^5.70.0"
